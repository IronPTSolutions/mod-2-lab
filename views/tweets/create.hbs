<main class="mb-3">
    <h1>New Tweet</h1>
    <form action="/tweets" method="post">
    <div class="mb-3">
    <label class="form-label">Message</label>
    <textarea name="message" id="message" class="form-control" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
    </form>
</main>